<p id="notice"><%= notice %></p>

<h1>Atencions</h1>

<table>
  <thead>
    <tr>
      <th>Clue</th>
      <th>Unidad</th>
      <th>Paciente</th>
      <th>Fecha</th>
      <th>Diagnosticos</th>
      <th>Tipoat</th>
      <th>Semanas</th>
      <th>Gesta</th>
      <th>Para</th>
      <th>Aborto</th>
      <th>Cesareas</th>
      <th>Terminacion</th>
      <th>Termino</th>
      <th>Altapor</th>
      <th>Fechaeg</th>
      <th>User</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atencions.each do |atencion| %>
      <tr>
        <td><%= atencion.clue_id %></td>
        <td><%= atencion.unidad %></td>
        <td><%= atencion.paciente_id %></td>
        <td><%= atencion.fecha %></td>
        <td><%= atencion.diagnosticos %></td>
        <td><%= atencion.tipoat %></td>
        <td><%= atencion.semanas %></td>
        <td><%= atencion.gesta %></td>
        <td><%= atencion.para %></td>
        <td><%= atencion.aborto %></td>
        <td><%= atencion.cesareas %></td>
        <td><%= atencion.terminacion %></td>
        <td><%= atencion.termino %></td>
        <td><%= atencion.altapor %></td>
        <td><%= atencion.fechaeg %></td>
        <td><%= atencion.user_id %></td>
        <td><%= atencion.tipo_id %></td>
        <td><%= link_to 'Show', atencion %></td>
        <td><%= link_to 'Edit', edit_atencion_path(atencion) %></td>
        <td><%= link_to 'Destroy', atencion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Atencion', new_atencion_path %>

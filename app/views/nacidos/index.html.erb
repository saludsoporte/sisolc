<p id="notice"><%= notice %></p>

<h1>Nacidos</h1>

<table>
  <thead>
    <tr>
      <th>Paciente</th>
      <th>Atencion</th>
      <th>Evolucion</th>
      <th>Atencion</th>
      <th>Egreso</th>
      <th>Defuncion</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nacidos.each do |nacido| %>
      <tr>
        <td><%= nacido.paciente_id %></td>
        <td><%= nacido.atencion_id %></td>
        <td><%= nacido.evolucion %></td>
        <td><%= nacido.atencion %></td>
        <td><%= nacido.egreso %></td>
        <td><%= nacido.defuncion %></td>
        <td><%= nacido.user_id %></td>
        <td><%= link_to 'Show', nacido %></td>
        <td><%= link_to 'Edit', edit_nacido_path(nacido) %></td>
        <td><%= link_to 'Destroy', nacido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nacido', new_nacido_path %>

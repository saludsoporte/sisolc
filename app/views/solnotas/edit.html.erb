<h1>Editing solnota</h1>

<% form_for(@solnota) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :solicitud_id %><br />
    <%= f.text_field :solicitud_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :fecha %><br />
    <%= f.date_select :fecha %>
  </p>
  <p>
    <%= f.label :comenta %><br />
    <%= f.text_area :comenta %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @solnota %> |
<%= link_to 'Back', solnotas_path %>
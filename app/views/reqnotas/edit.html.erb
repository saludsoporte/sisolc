<h1>Editing reqnota</h1>

<% form_for(@reqnota) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :requisicion_id %><br />
    <%= f.text_field :requisicion_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :fecha %><br />
    <%= f.date_select :fecha %>
  </p>
  <p>
    <%= f.label :comenta %><br />
    <%= f.text_area :comenta %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @reqnota %> |
<%= link_to 'Back', reqnotas_path %>
<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Nombre</th>
      <th>Puesto</th>
      <th>Area</th>
      <th>Rol</th>
      <th>Estructura</th>
      <th>Login</th>
      <th>Email</th>
      <th>Crypted password</th>
      <th>Salt</th>
      <th>Remember token</th>
      <th>Remember token expires at</th>
      <th>Dato1</th>
      <th>Dato2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.titulo %></td>
        <td><%= user.nombre %></td>
        <td><%= user.puesto %></td>
        <td><%= user.area_id %></td>
        <td><%= user.rol_id %></td>
        <td><%= user.estructura_id %></td>
        <td><%= user.login %></td>
        <td><%= user.email %></td>
        <td><%= user.crypted_password %></td>
        <td><%= user.salt %></td>
        <td><%= user.remember_token %></td>
        <td><%= user.remember_token_expires_at %></td>
        <td><%= user.dato1 %></td>
        <td><%= user.dato2 %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>

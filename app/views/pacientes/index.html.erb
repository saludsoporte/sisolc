<p id="notice"><%= notice %></p>

<h1>Pacientes</h1>

<table>
  <thead>
    <tr>
      <th>Paterno</th>
      <th>Materno</th>
      <th>Nombre</th>
      <th>Nacimiento</th>
      <th>Curp</th>
      <th>Observa</th>
      <th>Cambio</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.paterno %></td>
        <td><%= paciente.materno %></td>
        <td><%= paciente.nombre %></td>
        <td><%= paciente.nacimiento %></td>
        <td><%= paciente.curp %></td>
        <td><%= paciente.observa %></td>
        <td><%= paciente.cambio %></td>
        <td><%= paciente.user_id %></td>
        <td><%= link_to 'Show', paciente %></td>
        <td><%= link_to 'Edit', edit_paciente_path(paciente) %></td>
        <td><%= link_to 'Destroy', paciente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paciente', new_paciente_path %>

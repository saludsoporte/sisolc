<p id="notice"><%= notice %></p>

<h1>Clues</h1>

<table>
  <thead>
    <tr>
      <th>Clues</th>
      <th>Nomunidad</th>
      <th>Tipounidad</th>
      <th>Cvemuni</th>
      <th>Municipio</th>
      <th>Cveloc</th>
      <th>Localidad</th>
      <th>Domicilio</th>
      <th>Cp</th>
      <th>Estructura</th>
      <th>Nomabrev</th>
      <th>Juris</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clues.each do |clue| %>
      <tr>
        <td><%= clue.clues %></td>
        <td><%= clue.nomunidad %></td>
        <td><%= clue.tipounidad %></td>
        <td><%= clue.cvemuni %></td>
        <td><%= clue.municipio %></td>
        <td><%= clue.cveloc %></td>
        <td><%= clue.localidad %></td>
        <td><%= clue.domicilio %></td>
        <td><%= clue.cp %></td>
        <td><%= clue.estructura_id %></td>
        <td><%= clue.nomabrev %></td>
        <td><%= clue.juris_id %></td>
        <td><%= link_to 'Show', clue %></td>
        <td><%= link_to 'Edit', edit_clue_path(clue) %></td>
        <td><%= link_to 'Destroy', clue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clue', new_clue_path %>
